{
    "version": "0.2.0",
    "configurations": [
      {
        "name": ".NET 6 - ProAtividade.API",
        "type": "coreclr",
        "request": "launch",
        "preLaunchTask": "build",
        "program": "${workspaceFolder}/back/src/ProAtividade.API/bin/Debug/net6.0/ProAtividade.API.dll",
        "args": [],
        "cwd": "${workspaceFolder}/back/src/ProAtividade.API",
        "stopAtEntry": false,
        "serverReadyAction": {
          "action": "openExternally",
          "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
        },
        "env": {
          "ASPNETCORE_ENVIRONMENT": "Development"
        },
        "sourceFileMap": {
          "/Views": "${workspaceFolder}/Views"
        }
      }
      
    ]
  }
  